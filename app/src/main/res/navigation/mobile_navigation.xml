<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_maps">

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.bayurifki10120003.tugasgmaps.ui.profile.ProfileFragment"
        android:label="@string/title_home"
        tools:layout="@layout/maps" />

    <fragment
        android:id="@+id/navigation_maps"
        android:name="com.bayurifki10120003.tugasgmaps.ui.maps.MapsFragment"
        android:label="@string/title_map"
        tools:layout="@layout/profile" />

    <fragment
        android:id="@+id/navigation_information"
        android:name="com.bayurifki10120003.tugasgmaps.ui.information.InformationFragment"
        android:label="@string/title_info"
        tools:layout="@layout/information" />
</navigation>